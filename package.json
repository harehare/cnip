{
  "name": "cnip-snippet",
  "version": "0.1.17",
  "keywords": [
    "rescript",
    "cli",
    "snippet"
  ],
  "bin": {
    "cnip": "bin/cli.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harehare/cnip/tree/master"
  },
  "bugs": {
    "url": "https://github.com/harehare/cnip/issues"
  },
  "license": "MIT",
  "author": "harehare",
  "scripts": {
    "clean": "rescript clean",
    "dev": "rescript build -w",
    "format": "rescript format **/*.res",
    "build": "rescript && node build.js && terser --compress --mangle -- dist/index.js > dist/index.js2 && mv dist/index.js2 dist/index.js",
    "test": "rescript && jest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "clipboardy": "^2.3.0",
    "fast-fuzzy": "^1.12.0",
    "ink-text-input": "^4.0.3",
    "minimist": "^1.2.8",
    "toml": "^3.0.0",
    "uuid": "^9.0.1"
  },
  "files": [
    "dist",
    "bin",
    "images"
  ],
  "packageManager": "npm@10.2.4",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.10.0",
    "@rescript-ink/ink-link": "^3.0.0",
    "@rescript-ink/ink-spinner": "^3.0.0",
    "@rescript-ink/ink-text-input": "^3.0.0",
    "@rescript/core": "^0.7.0",
    "@rescript/react": "^0.12.1",
    "esbuild": "^0.20.0",
    "react": "^18.2.0",
    "rescript": "^11.0.1",
    "rescript-ink": "^1.3.0",
    "rescript-json": "^1.0.6",
    "rescript-nodejs": "^16.0.0",
    "rescript-struct": "^4.1.1",
    "rescript-uuid": "^9.0.1",
    "terser": "^5.27.1"
  }
}
